<?xml version="1.0" encoding="UTF-8"?>
<beans>

    <bean name="outputService" class="com.chenbuer.tinyioc.OutputService">
        <property name="helloWorldServiceImpl" ref="helloWorldServiceImpl"></property>
    </bean>

    <bean name="hw" class="com.chenbuer.tinyioc.HelloWorldServiceImpl">
        <property name="text" value="hw"></property>
    </bean>

    <bean name="helloWorldServiceImpl" class="com.chenbuer.tinyioc.HelloWorldServiceImpl">
        <property name="text" value="Hello World!"></property>
    </bean>

</beans>